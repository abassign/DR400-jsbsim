  <!-- L'helice --> 
  <animation>
    <name>HeliceComplete</name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
    <object-name>helice</object-name>
    <object-name>bol</object-name>
    <object-name>vis-bol</object-name>
  </animation>

  <!-- Selection de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdisc</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
         <value>2800</value><!--value>1500</value-->
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblur</object-name>
    <condition>
    <and>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>150</value><!--value>600.000</value-->
      </greater-than>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>1000</value><!--value>1500.001</value-->
      </less-than>
    </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>helice</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
	<value>150</value><!--value>1500</value-->
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>HeliceComplete</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -3.274 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.029 </z-m>
    </center>
    <axis>
      <x> -1.000 </x>
      <y>  0.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

 <!-- Gouverne de profondeur -->
 <animation>
  <type>rotate</type>
  <object-name>profondeur</object-name>
  <object-name>trimprof</object-name>
  <!--<property>controls/flight/elevator</property>-->
  <property>sim/multiplay/generic/float[2]</property>
  <interpolation>
    <entry><ind> -0.75 </ind><dep> -12.0 </dep></entry>
    <entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
    <entry><ind>  0.75 </ind><dep>   9.5 </dep></entry>
  </interpolation>
  <center>
   <x-m> 2.543 </x-m>
   <y-m> 0.000 </y-m>
   <z-m> 0.154 </z-m>
  </center>
  <axis>
   <x> 0.000 </x>
   <y> 1.000 </y>
   <z> 0.000 </z>
  </axis>
 </animation>

  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>direction</object-name>
    <!--<property>controls/flight/rudder</property>-->
    <property>sim/multiplay/generic/float[3]</property>
    <interpolation>
      <entry><ind> -0.75 </ind><dep> -25.0 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  0.75 </ind><dep>  25.0 </dep></entry>
    </interpolation>
    <axis>
      <x1-m>  2.824 </x1-m>
      <y1-m>  0.000 </y1-m>
      <z1-m> -0.200 </z1-m>
      <x2-m>  3.062 </x2-m>
      <y2-m>  0.000 </y2-m>
      <z2-m>  1.113 </z2-m>
    </axis>
  </animation>

  <!-- Animations des ailerons -->
  <animation>
    <type>rotate</type>
    <object-name>aileronG</object-name>
    <!--<property>controls/flight/aileron</property>-->
    <property>sim/multiplay/generic/float[1]</property>
    <interpolation>
      <entry><ind> -0.75 </ind><dep>  15.0 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  0.75 </ind><dep> -10.0 </dep></entry>
    </interpolation>
       <axis>
      <x1-m> -0.457 </x1-m>
      <y1-m> -2.752 </y1-m>
      <z1-m> -0.382 </z1-m>
      <x2-m> -1.058 </x2-m>
      <y2-m> -4.331 </y2-m>
      <z2-m>  0.006 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>tringleaileronG</object-name>
    <!--<property>controls/flight/aileron</property>-->
    <property>sim/multiplay/generic/float[1]</property>
    <factor>0.008</factor>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
